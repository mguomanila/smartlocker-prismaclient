datasource db{
  provider =  "postgresql"
  url =  env("DATABASE_URL")
}

generator client {
  provider =  "prisma-client-js"
}

model User {
  id      Int  @id @default(autoincrement())
  name    String?
  email   String  @unique
}

enum LockerType {
  LockerType1
  LockerType2
  LockerType3
}

model LockerTimeLimits {
  id Int @id @default(autoincrement())
  lockerType LockerType
  pickupTimeLimit Int
  reclaimTimeLimit Int
  bookingExpiry Int
  shipoutTimeLimit Int
  storageMaxTimeLimit Int
}

model LockerUserTypes {
  userType String
  Description String
  MonthlyFee String
}

model CourierTypes {
  LockerType
  CourierType
  Description
  MonthlyFee
}

model SellerTypes {
  LockerType
  SellerType
  Description
  MonthlyFee
}

model MessageTemplates {
  LockerType
  SendAlertVia
  SMSMessage
  AppMessageSameAsSMS
  AppMessage
}

model LockerFees {
  LockerType
  UserType
  CourierType
  SellerType
  DoorSizes
  InitialPickupFee
  InitialReturnCancellationFee
  InitialShipoutFee
  StorageFee
  DropoffFee
  InitialFreeHours
  AdditionalOverstayChargesAfterInitialFreeHoursFee
  AdditionalOverstayChargesAfterInitialFreeHoursEveryHours
  StorageDuration
  MaxOverstayCharge
  CommissionPercentageOnShipoutAmount
}

model AssignLockerToAgent {
  Lockers
  Courier
  Branch
  From
  To
  Agent
}

model AssignLockerToBranch {
  Lockers
  Courier
  Branch
}