datasource db{
  provider =  "postgresql"
  url =  env("DATABASE_URL")
}

generator client {
  provider =  "prisma-client-js"
}

model User {
  id      Int  @id @default(autoincrement())
  name    String?
  email   String  @unique
}

enum LockerType {
  LockerType1
  LockerType2
  LockerType3
}

model LockerTimeLimits {
  id Int @id @default(autoincrement())
  lockerType LockerType
  pickupTimeLimit Int
  reclaimTimeLimit Int
  bookingExpiry Int
  shipoutTimeLimit Int
  storageMaxTimeLimit Int
}
